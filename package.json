{
  "private": true,
  "name": "domotics-dashboard",
  "version": "0.0.1",
  "description": "A customizable front to manage domotics with Myfox services",
  "author": "Xavier Gouley <gxapplications@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "test": "cross-env NODE_ENV=test mocha",
    "doc": "rimraf doc && jsdoc --package package.json --destination doc/jsdoc/ server/ lib/ && groc && cross-env NODE_ENV=test nyc --require babel-core/register --reporter=lcov mocha && rimraf .nyc_output",
    "doc:publish": "npm run doc && gh-pages -d doc",
    "dev:webpack": "webpack --progress --colors",
    "webpack": "webpack --progress --colors --optimize-minimize",
    "lint": "standard --verbose | snazzy",
    "validate": "npm run webpack && npm run lint && npm test && npm ls",
    "dev:start": "npm run dev:webpack && nodemon index.js",
    "dev:test": "npm run test -- --watch",
    "debug:start": "iron-node index.js",
    "debug:test": "cross-env NODE_ENV=test iron-node node_modules/mocha/bin/_mocha"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/gxapplications/domotics-dashboard"
  },
  "bugs": {
    "url": "https://github.com/gxapplications/domotics-dashboard/issues"
  },
  "homepage": "https://github.com/gxapplications/domotics-dashboard",
  "keywords": [
    "myfox",
    "domotics",
    "dashboard"
  ],
  "engines": {
    "node": ">= 5.0.0",
    "npm": ">= 3.6.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "standard": {
    "env": {
      "node": true,
      "mocha": true
    },
    "parser": "babel-eslint",
    "ignore": [
      "/doc/",
      "/public/"
    ]
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "nyc": {
    "lines": 40,
    "check-coverage": true,
    "report-dir": "./doc/coverage",
    "include": [
      "lib/**",
      "server/**"
    ]
  },
  "dependencies": {
    "angular": "^1.5.6",
    "angular-animate": "^1.5.6",
    "angular-aria": "^1.5.6",
    "angular-material": "1.1.0-rc.5",
    "angular-messages": "^1.5.6",
    "assign-deep": "^0.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.5.0",
    "colors": "^1.1.2",
    "config": "^1.21.0",
    "crypto-js": "^3.1.6",
    "handlebars": "^4.0.5",
    "handlebars-extend-block": "^1.0.0",
    "hapi": "^13.4.1",
    "hapi-qs": "^1.1.1",
    "hoek": "^4.0.1",
    "inert": "^4.0.0",
    "jquery": "^3.0.0",
    "lodash": "^4.14.1",
    "myfox-wrapper-api": "^0.0.6",
    "nes": "^6.2.1",
    "path": "^0.12.7",
    "sqlite3": "^3.1.4",
    "vision": "^4.1.0",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cross-env": "^1.0.7",
    "gh-pages": "^0.11.0",
    "groc": "^0.8.0",
    "iron-node": "^2.2.16",
    "jsdoc": "^3.4.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.1",
    "nyc": "^6.1.1",
    "precommit-hook": "^3.0.0",
    "rimraf": "^2.5.2",
    "standard": "^6.0.1",
    "supertest": "^1.2.0"
  },
  "directories": {
    "doc": "./doc",
    "lib": "./lib"
  },
  "config": {}
}
